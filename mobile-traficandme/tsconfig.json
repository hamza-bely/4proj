{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@core/*": ["app/core/*"],
      "@assets/*":["assets/*"],
      "@supabase":["app/core/services/supabase.js"],
      "@components/*":["components/*"],
      "@interfaces/*":["app/interfaces/*"],
      "@services/*":["app/core/services/*"],
      "@utils/*":["app/utils/*"],
      
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/core/services/supabase.js", "app/core/services/apiService.ts"  ]
}
